{
  "name": "wuffel",
  "version": "0.0.1",
  "description": "A all purpose discord bot written with discord.js",
  "main": "index.ts",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "nodemon dist/index.js",
    "deploy": "node dist/Slash-Commands-Deployer.js",
    "create:migration": "mikro-orm migration:create",
    "gen-env": "npx gen-env-types .env -o env.d.ts -e ."
  },
  "keywords": [
    "wuffel"
  ],
  "author": "Shiny",
  "license": "ISC",
  "dependencies": {
    "@babel/helper-compilation-targets": "^7.18.2",
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@mikro-orm/core": "^5.2.0",
    "@mikro-orm/postgresql": "^5.2.0",
    "discord-api-types": "^0.33.5",
    "discord.js": "^13.8.0",
    "dotenv-safe": "^8.2.0",
    "format-duration": "^2.0.0",
    "moment": "^2.29.3",
    "pg": "^8.7.3",
    "pretty-ms": "^8.0.0",
    "statman-stopwatch": "^2.13.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-config.ts",
      "./dist/mikro-config.js"
    ]
  }
}
